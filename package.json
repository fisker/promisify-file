{
  "name": "promisify-file",
  "version": "5.0.0",
  "description": "Promise File Read.",
  "main": "lib/index.js",
  "browser": "lib/index.min.js",
  "module": "lib/index.min.mjs",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "keywords": [
    "promise",
    "file",
    "fileReader"
  ],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf dist",
    "dev": "rollup --config --watch",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\" --format friendly",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint clean build format release:*",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "repository": "fisker/promise-file",
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@fisker/commitlint-config": "^1.1.2",
    "@fisker/eslint-config": "^1.2.15",
    "@fisker/eslint-config-jest": "^1.1.9",
    "@fisker/husky-config": "^1.1.1",
    "@fisker/lint-staged-config": "^1.0.5",
    "@fisker/prettier-config": "^1.0.18",
    "babel-jest": "24.9.0",
    "babel-plugin-transform-async-to-promises": "^0.8.14",
    "babel-polyfill": "6.26.0",
    "babel-preset-minify": "0.5.1",
    "canvas": "^2.6.0",
    "core-js": "3.2.1",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "6.2.2",
    "eslint-formatter-friendly": "^7.0.0",
    "husky": "^3.0.4",
    "jest": "24.9.0",
    "jest-environment-jsdom-thirteen": "^1.0.1",
    "lint-staged": "^9.2.5",
    "markdownlint-cli": "^0.18.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "1.12.3",
    "prettier": "1.18.2",
    "promisify-file-reader": "^3.0.4",
    "regenerator-runtime": "0.13.3",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-prettier": "^0.6.0",
    "rollup-plugin-terser": "^5.1.1",
    "uglify-js": "3.6.0",
    "whatwg-fetch": "3.0.0"
  },
  "files": [
    "lib/index.js"
  ],
  "bugs": {
    "url": "https://github.com/fisker/promise-file/issues"
  },
  "homepage": "https://github.com/fisker/promise-file#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "sideEffects": false
}
