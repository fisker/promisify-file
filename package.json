{
  "name": "promisify-file",
  "version": "5.0.0",
  "description": "Promise File Read.",
  "main": "lib/index.js",
  "browser": "lib/index.min.js",
  "module": "lib/index.min.mjs",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "keywords": [
    "promise",
    "file",
    "fileReader"
  ],
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf dist",
    "dev": "rollup --config --watch",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\" --format friendly",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint clean build format release:*",
    "test": "jest",
    "test-coverage": "jest --coverage"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "MIT",
  "repository": "fisker/promise-file",
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.6",
    "@fisker/eslint-config-jest": "1.1.13",
    "@fisker/husky-config": "1.1.4",
    "@fisker/lint-staged-config": "1.0.6",
    "@fisker/prettier-config": "1.0.22",
    "babel-jest": "24.9.0",
    "babel-plugin-transform-async-to-promises": "0.8.15",
    "babel-polyfill": "6.26.0",
    "babel-preset-minify": "0.5.1",
    "canvas": "2.6.0",
    "core-js": "3.4.5",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "6.7.1",
    "eslint-formatter-friendly": "7.0.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-thirteen": "1.0.1",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "promisify-file-reader": "3.0.4",
    "regenerator-runtime": "0.13.3",
    "rollup": "1.27.5",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.1.2",
    "uglify-js": "3.7.0",
    "whatwg-fetch": "3.0.0"
  },
  "files": [
    "lib/index.js"
  ],
  "bugs": {
    "url": "https://github.com/fisker/promise-file/issues"
  },
  "homepage": "https://github.com/fisker/promise-file#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "sideEffects": false
}
