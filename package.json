    "lint-staged": "^8.1.7",
    "markdownlint-cli": "^0.16.0",
    "parcel-bundler": "1.12.3",
    "prettier": "1.17.1",
    "regenerator-runtime": "0.13.2",
    "uglify-js": "3.5.15",
    "whatwg-fetch": "3.0.0"
